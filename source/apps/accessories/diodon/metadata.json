        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "yakkety"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "diodon",
            "install-packages": [
                "diodon"
            ],
            "remove-packages": [
                "diodon"
            ],
            "source": "universe"
        }
    }
