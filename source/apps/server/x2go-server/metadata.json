{
    "listed": true,
    "name": "X2Go Server",
    "summary": "Allows you to connect and control an instance of your desktop remotely.",
    "developer-name": "Various Contributors",
    "developer-url": "http://wiki.x2go.org/doku.php/about:start",
    "description": "A remote computing environment with support for session resuming, low bandwidth support, session brokerage, client-side mass storage mounting, client-side printing, audio redirection and authentication by smartcard or USB stick. ",
    "tags": "Server",
    "launch-cmd": null,
    "proprietary": false,
    "alternate-to": "Citrix Workspace Cloud",
    "urls": {
        "info": "http://wiki.x2go.org/doku.php",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64"
    ],
    "releases": [
        "xenial",
        "yakkety",
        "zesty"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "x2goserver",
            "install-packages": [
                "x2goserver",
                "x2goserver-extensions",
                "x2goserver-xsession",
                "x2gomatebindings"
            ],
            "remove-packages": [
                "x2goserver",
                "x2goserver-extensions",
                "x2goserver-xsession",
                "x2gomatebindings"
            ],
            "source": "ppa:x2go/stable"
        }
    }
}
