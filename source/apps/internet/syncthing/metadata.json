{
    "listed": true,
    "name": "Syncthing",
    "summary": "Your own decentralized sync and cloud service",
    "developer-name": "Various Contributors",
    "developer-url": "https://github.com/syncthing/syncthing/blob/master/AUTHORS",
    "description": "Syncthing replaces proprietary sync and cloud services with something open, trustworthy and decentralized. Your data is your data alone and you deserve to choose where it is stored, if it is shared with some third party and how it&#8217;s transmitted over the Internet. ",
    "tags": "Cloud and Sync",
    "launch-cmd": "syncthing",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "https://syncthing.net/",
        "android-app": "https://play.google.com/store/apps/details?id=com.nutomic.syncthingandroid",
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf"
    ],
    "releases": [
        "xenial",
        "yakkety",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "syncthing",
            "install-packages": [
                "syncthing"
            ],
            "remove-packages": [
                "syncthing"
            ],
            "source": "manual",
            "list-file": "syncthing.list",
            "list-key-url": "https://syncthing.net/release-key.txt"
        }
    }
}
